<div class="header-holder accountpage clearfix" style=" position: relative; 
    margin-left: auto;
    margin-right: auto;
    padding-left: 0px;
    padding-right: 0px;
    max-width: 1100px;">
  <div class="menu-btn">
    <div class="navi-icon">
      <span></span>
      <span></span>
      <span></span>
      <span></span>
    </div>
  </div>
  <div class="logo">
    <%=link_to root_path do  %><%=image_tag "logo.svg",alt:"logo" %> <%end%>
  </div>
  <div class="navigation-holder">
    <ul class="navigation" style="    bottom: 35px;
    position: absolute;
    right: 0px;">
      <li><%=link_to "Home",root_path %></li>
      <%if user_signed_in?  %>
        <li><%=link_to "Dashboard",admin_root_path %></li>
      <%end %>
      <li><%= link_to "Contact",contact_path %></li>
      <%if current_user %>
        <li><%=link_to "Logout",destroy_user_session_path, method: :delete,class:"active" %></li>
      <%else %>
        <li><%=link_to "Register",plans_path %></li>
        <li><%=link_to "Login" , new_user_session_path ,:class => 'login-btn'%></li>
      <%end %>
    </ul>
  </div>
</div>